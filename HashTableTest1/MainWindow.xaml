<Window x:Class="HashTableTest1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashTableTest1"
        mc:Ignorable="d"
        Title="HashTableTest" Height="450" Width="800">
    <Grid>
        <Button x:Name="addBtn" Content="Add" HorizontalAlignment="Left" Height="43" Margin="168,259,0,0" VerticalAlignment="Top" Width="133" FontSize="18" Click="addBtn_Click"/>
        <Button x:Name="removeBtn" Content="Remove" HorizontalAlignment="Left" Height="43" Margin="341,259,0,0" VerticalAlignment="Top" Width="133" FontSize="18" Click="removeBtn_Click"/>
        <Button x:Name="showBtn" Content="Show" HorizontalAlignment="Left" Height="43" Margin="513,259,0,0" VerticalAlignment="Top" Width="133" FontSize="18" Click="showBtn_Click"/>
        <TextBox x:Name="valTxt" HorizontalAlignment="Left" Height="41" Margin="168,182,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="478" FontSize="16"/>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Height="41" Margin="168,118,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="478" FontSize="16"/>

    </Grid>
</Window>
